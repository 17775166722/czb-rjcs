          <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="JSR223 Sampler" enabled="true">
            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">import redis.clients.jedis.Jedis;
import java.io.ByteArrayInputStream;
import java.io.ObjectInputStream;

// Redis 连接信息
String redisHost = &quot;localhost&quot;; // Redis 地址
int redisPort = 6379;          // Redis 端口
String redisPassword = &quot;Shadingyu123&quot;; // Redis 密码
String redisKey = &quot;login_captcha_key_${sn}&quot;; // 存储验证码的 Redis Key

// 创建 Redis 客户端连接
Jedis jedis = new Jedis(redisHost, redisPort);
if (redisPassword != null &amp;&amp; !redisPassword.isEmpty()) {
    jedis.auth(redisPassword);
}

// 从 Redis 获取序列化的值
byte[] serializedValue = jedis.get(redisKey.getBytes());

// 反序列化
String captchaCode = null;
if (serializedValue != null) {
    try (ByteArrayInputStream bais = new ByteArrayInputStream(serializedValue);
         ObjectInputStream ois = new ObjectInputStream(bais)) {
        captchaCode = (String) ois.readObject(); // 假设存储的对象是字符串
    } catch (Exception e) {
        log.error(&quot;反序列化失败: &quot; + e.getMessage());
    }
}

// 将验证码存入 JMeter 变量
if (captchaCode != null) {
    vars.put(&quot;captchaCode&quot;, captchaCode);
    log.info(&quot;验证码提取成功: &quot; + captchaCode);
} else {
    log.error(&quot;未能提取到验证码&quot;);
}</stringProp>
          </JSR223Sampler>
